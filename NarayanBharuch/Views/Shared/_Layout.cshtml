    <script src="~/Content/Js/jquery-ui.min.js"></script>
    @*<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jqueryui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js"></script>*@
    <script src="~/Content/Js/jquery.ui.touch-punch.min.js"></script>
    <script src="~/Scripts/modernizr.custom.js"></script>
    <script src="~/Scripts/classie.js"></script>
    <script src="~/Scripts/modalEffects.js"></script>
    <script src="~/Scripts/sweetalert.min.js"></script>
    <script src="~/Scripts/attrvalidate.jquery.js"></script>
</head>
<body>
    <header>
        <div class="container">
            <!-- Logo -->
            <div class="logo">
                <h1><a href="#"><img src="@ViewBag.AppLogo" title="@ViewBag.AppName" alt="@ViewBag.AppName"></a></h1>
            </div>
        </div>
    </header>
    @RenderBody()
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <aside id="execphp-3" class="widget widget_execphp">
                        <h1 class="widget-title">@ViewBag.FooterText</h1>
                        <div class="execphpwidget">
                            <div class="addressline">@ViewBag.Address</div>
                            @*<ul class="social">
                                <li><a href="#" title="Facebook" target="_blank"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                <li><a href="#" title="Tweeter" target="_blank"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                <li><a href="#" title="Linkedin" target="_blank"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                                <li><a href="#" title="Linkedin" target="_blank"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a></li>
                                <li><a href="#" title="Linkedin" target="_blank"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                            </ul>*@
                        </div>
                    </aside>
                </div>
            </div>
        </div>
        <div class="copyright">
            <div class="container">
                All Rights Reserved by &copy; @DateTime.Now.Year @ViewBag.AppName.
            </div>
        </div>
    </footer>
    @*<script src="http://108.179.197.185:59/wp-content/themes/bezeltech/js/jquery.min.js"></script>
